{
  "name": "angulatransmission",
  "version": "0.0.1",
  "default_locale": "en",
  "manifest_version": 2,
  "background": {"persistent": false, "scripts": ["js/angular.js", "js/angular-base64.js", "js/session.js", "js/backgroundapp.js", "js/backgroundmain.js"]},
  "browser_action": {
    "default_title": "Transmission",
    "default_popup": "main.html"
  },

  "permissions": [
    "<all_urls>",
    "contextMenus",
    "downloads",
    "downloads.open"
  ]
}
